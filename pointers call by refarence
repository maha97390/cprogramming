// pointer using functions
#include<stdio.h>
#include<conio.h>
int main()
{
    int a=10,b=20,*p,*q;
    p=&a;
    q=&b;
    printf("%d and %d\n",a,b);
    printf("%p\n",p);
    printf("%p\n",q);
    swap(&a,&b);
    printf("%d and %d\n",a,b);
    printf("%p\n",p);
    printf("%p\n",q);
    return 0;

}
int swap(int *n,int *m)
{
    int temp;
    temp=*n;
    *n=*m;
    *m=temp;
    return n,m;
}
