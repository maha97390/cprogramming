#include<stdio.h>
#include<conio.h>
void main()
{
    int a,b,c;
    printf("Enter three numbers:");
    scanf("%d%d%d",&a,&b,&c);
    if(a>b && a>c)
    {
        if(b>c)
        {
            printf("second largest is: %d",b);
        }
        else
        {
            printf("second largest is: %d",c);
        }
    }
    else if(b>a && b>c)
    {
        if(a>c)
        {
            printf("second largest is: %d",a);
        }
        else
        {
            printf("second largest is: %d",c);
        }
    }
    if(c>b && c>a)
    {
        if(b>a)
        {
            printf("second largest is: %d",b);
        }
        else
        {
            printf("second largest is: %d",a);
        }
    }
}
